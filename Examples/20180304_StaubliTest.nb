Cell["\<\



Initialize the inverse kinematics.\
\>", "Text",
 Evaluatable->False,
 AspectRatioFixed->True],

Cell["\<\
theta1 = ArcTan[X, Y];

theta3i = ArcSin[(-d2^2 - d5^2 + X^2 + Y^2 + Z^2)/(2*d2*d5)];

theta2 = ArcTan[(Csc[t1]*(d2*Y + d5*Z*Cos[t3]*Sin[t1] + d5*Y*Sin[t3]))/ 
   (d2^2 + d5^2 + 2*d2*d5*Sin[t3]), 
   -((2*d2*X^2*Z + 2*d2*Y^2*Z + 2*d2*Z^3 - 2*d2*d5^2*Z*Cos[t3]^2 - 
        d2^2*d5*Y*Cos[t3]*Csc[t1] + d5^3*Y*Cos[t3]*Csc[t1] - 
        d5*X^2*Y*Cos[t3]*Csc[t1] - d5*Y^3*Cos[t3]*Csc[t1] - 
        d5*Y*Z^2*Cos[t3]*Csc[t1])/
      ((X^2 + Y^2 + Z^2)*(d2^2 - d5^2 + X^2 + Y^2 + Z^2)))];

theta3 = ArcTan[(Csc[t1]*(Z*Cos[t2]*Sin[t1] + Y*Sin[t2]))/d5, 
   (-d2^2 - d5^2 + X^2 + Y^2 + Z^2)/(2*d2*d5)];

theta4i = ArcTan[-((Csc[t2 + t3]*(a23*Cos[t1] - a13*Sin[t1]))/
     (a33*Cos[t1]^2 - a13*Cos[t1]*Cot[t2 + t3] - 
       a23*Cot[t2 + t3]*Sin[t1] + a33*Sin[t1]^2))];
       
theta5=ArcTan[-(-(a21*Cos[t1 - t2 - t3]) - 2*a31*Cos[t2 + t3] + 
      a21*Cos[t1 + t2 + t3] + a11*Sin[t1 - t2 - t3] - 
      a11*Sin[t1 + t2 + t3])/
   (2*(-(a11*Cos[t1]*Cos[t2 + t3]*Cos[t4]) - 
       a21*Cos[t2 + t3]*Cos[t4]*Sin[t1] + 
       a31*Cos[t1]^2*Cos[t4]*Sin[t2 + t3] + 
       a31*Cos[t4]*Sin[t1]^2*Sin[t2 + t3] - 
       a21*Cos[t1]*Cos[t2 + t3]^2*Sin[t4] + 
       a11*Cos[t2 + t3]^2*Sin[t1]*Sin[t4] - 
       a21*Cos[t1]*Sin[t2 + t3]^2*Sin[t4] + 
       a11*Sin[t1]*Sin[t2 + t3]^2*Sin[t4]))];

theta4 = ArcTan[-((Csc[t5]*(a33*Cos[t1]^2 - a13*Cos[t1]*Cot[t2 + t3] - 
          a23*Cot[t2 + t3]*Sin[t1] + a33*Sin[t1]^2))/
      (Cos[t2 + t3]*Cot[t2 + t3] + Sin[t2 + t3])), 
   Csc[t5]*(a23*Cos[t1] - a13*Sin[t1])];

theta6 = ArcTan[-(a22*Cos[t1]*Cos[t4]) + a12*Cos[t4]*Sin[t1] + 
    a12*Cos[t1]*Cos[t2 + t3]*Sin[t4] + 
    a22*Cos[t2 + t3]*Sin[t1]*Sin[t4] - a32*Sin[t2 + t3]*Sin[t4], 
   -(a21*Cos[t1]*Cos[t4]) + a11*Cos[t4]*Sin[t1] + 
    a11*Cos[t1]*Cos[t2 + t3]*Sin[t4] + 
    a21*Cos[t2 + t3]*Sin[t1]*Sin[t4] - a31*Sin[t2 + t3]*Sin[t4]];\
\>", "Input",
 AspectRatioFixed->True],

